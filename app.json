        "android.permission.DETECT_SCREEN_CAPTURE"
      ],
      "package": "com.manostattoos.ManosTattoos"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "729276a6-d210-485d-9ffe-83f1804687f2"
      }
    },
    "owner": "minaihab"
